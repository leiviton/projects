{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Software de Gerenciamento de atendimento a clientes.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "appointer/swaggervel": "^2.4",
        "barryvdh/laravel-cors": "^0.11.3",
        "comtele/comtele_sdk": "^1.1",
        "fideloper/proxy": "^4.0",
        "geekcom/validator-docs": "^1.5",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "league/fractal": "^0.18.0",
        "owen-it/laravel-auditing": "^9.2",
        "prettus/l5-repository": "^2.6",
        "reliese/laravel": "^0.0.13",
        "ext-soap": "*",
        "ext-simplexml": "*",
        "ext-calendar": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^4.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "ApiWebPsp\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
